#!../sapi/cli/php
<?php

namespace Mgrunder\Fuzzer;

require_once __DIR__ . '/' . '../vendor/autoload.php';

use Mgrunder\Fuzzer\Cmd\get;


$cfg = new FuzzConfig('localhost', 8080, 10, 10);
$reg = new Cmd\Registry($cfg);

for ($i = 0; $i < 5; $i++) {
    $cmd = $reg->randomCmd();
    print_r($cmd->fuzz());
}
